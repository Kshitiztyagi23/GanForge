      "source": [
        "# ✅ STEP-BY-STEP: FACE MASK DETECTION USING YOLOv5\n",
        "\n",
        "# =====================\n",
        "# STEP 1: SETUP YOLOv5\n",
        "# =====================\n",
        "!git clone https://github.com/ultralytics/yolov5\n",
        "%cd yolov5\n",
        "%pip install -r requirements.txt\n",
        "\n",
        "\n",
        "# ============================\n",
        "# STEP 2: UPLOAD DATA ZIP FILE\n",
        "# ============================\n",
        "from google.colab import files\n",
        "uploaded = files.upload()  # Upload facedetection.zip (from Kaggle)\n",
        "\n",
        "\n",
        "# =============================\n",
        "# STEP 3: UNZIP AND PREPARE XML\n",
        "# =============================\n",
        "import zipfile, os\n",
        "import xml.etree.ElementTree as ET\n",
        "from sklearn.model_selection import train_test_split\n",
        "import shutil\n",
        "\n",
        "# Unzip dataset\n",
        "zip_path = '/content/facedetection.zip'\n",
        "extract_path = '/content/facedetection'\n",
        "with zipfile.ZipFile(zip_path, 'r') as zip_ref:\n",
        "    zip_ref.extractall(extract_path)\n",
        "\n",
        "# Define paths\n",
        "annotations_dir = os.path.join(extract_path, 'facedetection', 'annotations')\n",
        "images_dir = os.path.join(extract_path, 'facedetection', 'images')\n",
        "labels_dir = os.path.join(extract_path, 'yolo_labels')\n",
        "os.makedirs(labels_dir, exist_ok=True)\n",
        "\n",
        "# Label map\n",
        "label_map = {\"with_mask\": 0, \"without_mask\": 1, \"mask_weared_incorrect\": 2}\n",
        "\n",
        "# Convert XML to YOLO format\n",
        "def convert(xml_file):\n",
        "    tree = ET.parse(xml_file)\n",
        "    root = tree.getroot()\n",
        "    size = root.find(\"size\")\n",
        "    w, h = int(size.find(\"width\").text), int(size.find(\"height\").text)\n",
        "    result = []\n",
        "    for obj in root.findall(\"object\"):\n",
        "        name = obj.find(\"name\").text\n",
        "        if name not in label_map:\n",
        "            continue\n",
        "        cid = label_map[name]\n",
        "        b = obj.find(\"bndbox\")\n",
        "        xmin = int(b.find(\"xmin\").text)\n",
        "        ymin = int(b.find(\"ymin\").text)\n",
        "        xmax = int(b.find(\"xmax\").text)\n",
        "        ymax = int(b.find(\"ymax\").text)\n",
        "        xc = ((xmin + xmax) / 2) / w\n",
        "        yc = ((ymin + ymax) / 2) / h\n",
        "        bw = (xmax - xmin) / w\n",
        "        bh = (ymax - ymin) / h\n",
        "        result.append(f\"{cid} {xc:.6f} {yc:.6f} {bw:.6f} {bh:.6f}\")\n",
        "    return result\n",
        "\n",
        "# Write label files\n",
        "for xml_file in os.listdir(annotations_dir):\n",
        "    if not xml_file.endswith(\".xml\"): continue\n",
        "    labels = convert(os.path.join(annotations_dir, xml_file))\n",
        "    txt_file = os.path.join(labels_dir, xml_file.replace(\".xml\", \".txt\"))\n",
        "    with open(txt_file, \"w\") as f:\n",
        "        f.write(\"\\n\".join(labels))\n",
        "\n",
        "\n",
        "# ==================================\n",
        "# STEP 4: ORGANIZE DATA FOR YOLOv5\n",
        "# ==================================\n",
        "image_files = sorted([f for f in os.listdir(images_dir) if f.endswith(\".png\")])\n",
        "pairs = [(os.path.join(images_dir, img), os.path.join(labels_dir, img.replace(\".png\", \".txt\")))\n",
        "         for img in image_files]\n",
        "\n",
        "train, val = train_test_split(pairs, test_size=0.2, random_state=42)\n",
        "\n",
        "# Create YOLOv5 structure\n",
        "base = \"/content/yolov5_dataset\"\n",
        "for t in [\"train\", \"val\"]:\n",
        "    os.makedirs(f\"{base}/images/{t}\", exist_ok=True)\n",
        "    os.makedirs(f\"{base}/labels/{t}\", exist_ok=True)\n",
        "\n",
        "# Copy files\n",
        "for img, lbl in train:\n",
        "    shutil.copy(img, f\"{base}/images/train/{os.path.basename(img)}\")\n",
        "    shutil.copy(lbl, f\"{base}/labels/train/{os.path.basename(lbl)}\")\n",
        "\n",
        "for img, lbl in val:\n",
        "    shutil.copy(img, f\"{base}/images/val/{os.path.basename(img)}\")\n",
        "    shutil.copy(lbl, f\"{base}/labels/val/{os.path.basename(lbl)}\")\n",
        "\n",
        "\n",
        "# ===========================\n",
        "# STEP 5: CREATE data.yaml\n",
        "# ===========================\n",
        "yaml = '''\n",
        "train: /content/yolov5_dataset/images/train\n",
        "val: /content/yolov5_dataset/images/val\n",
        "\n",
        "nc: 3\n",
        "names: ['with_mask', 'without_mask', 'mask_weared_incorrect']\n",
        "'''\n",
        "\n",
        "with open('/content/yolov5_dataset/data.yaml', 'w') as f:\n",
        "    f.write(yaml)\n",
        "\n",
        "\n",
        "# ===========================\n",
        "# STEP 6: TRAIN YOLOv5 MODEL\n",
        "# ===========================\n",
        "!python train.py --img 640 --batch 4 --epochs 30 --data /content/yolov5_dataset/data.yaml --weights yolov5s.pt --cache\n"
      ]
    }
  ]
}